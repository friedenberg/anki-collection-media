{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "require-jsdoc": 0,
        "new-cap": ["error", { "capIsNewExceptionPattern": "MathJax" }],
        "max-len": [
          "error",
          {
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true,
            "ignoreRegExpLiterals": true
          }
        ]
    }
}
